{
	"name": "mo-mediawiki/skins-common",
	"description": "A set of common resources for MediaWiki skins a free software wiki application developed by the Wikimedia Foundation and others",
	"type": "mediawiki-library",
	"keywords": [
		"mediawiki",
		"wiki",
		"skin",
		"library"
	],
	"require": {
		"php": ">=7.4.3",
		"composer/installers": ">1.0.12"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"installer-name": "SkinsCommon"
	},
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "41.0.0",
		"mediawiki/mediawiki-phan-config": "0.14.0",
		"mediawiki/minus-x": "1.1.1",
		"php-parallel-lint/php-console-highlighter": "1.0.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2"
	},
	"homepage": "https://www.mediawiki.org/",
	"authors": [
		{
			"name": "Mohd Abualruz",
			"email": "mabualruz@wikimedia.org"
		}
	],
	"license": "Apache-2.0",
	"support": {
		"issues": "https://phabricator.wikimedia.org/",
		"irc": "irc://irc.libera.chat/mediawiki",
		"wiki": "https://www.mediawiki.org/"
	},
	"autoload": {
		"psr-4": {
			"Mediawiki\\Skins\\Common\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Mediawiki\\Skins\\Common\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"test": [
			"parallel-lint . --exclude node_modules --exclude vendor",
			"@phpcs",
			"minus-x check ."
		],
		"phan": "phan -d . --long-progress-bar",
		"phpcs": "phpcs -sp --cache"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"wikimedia/composer-merge-plugin": false
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
